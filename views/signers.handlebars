<div id="signers-container">
    <p>Signers so far:</p>

    {{#if render}}
    
<table>
    <tr>
            <td><b>Name</b></td>
            <td><b>Age</b></td>
            <td><b>City</b></td>
        </tr>
    {{#rows}}

        
        <tr>
            {{{showAnchorUser url fullname}}}
            <td>{{age}}</td>
            <td><a href="/signers/{{city}}">{{city}}</a></td>
        </tr>

   
    {{/rows}}
     </table>
    {{else}}
    {{error}}

    {{/if}}
</div>